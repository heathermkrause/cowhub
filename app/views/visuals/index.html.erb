<h1>Listing visuals</h1>

<table>
  <tr>
    <th>Vizname</th>
    <th>Topic</th>
    <th>User</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @visuals.each do |visual| %>
  <tr>
    <td><%= visual.vizname %></td>
    <td><%= visual.topic %></td>
    <td><%= visual.user_id %></td>
    <td><%= link_to 'Show', visual %></td>
    <td><%= link_to 'Edit', edit_visual_path(visual) %></td>
    <td><%= link_to 'Destroy', visual, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Visual', new_visual_path %>
